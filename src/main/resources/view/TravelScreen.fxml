<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXHamburger?>
<?import com.jfoenix.controls.JFXTextField?>
<?import com.jfoenix.controls.JFXTimePicker?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="435.0" prefWidth="753.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tubeproject.view.TravelScreen">
    <children>
        <Pane fx:id="topPane" prefHeight="62.0" prefWidth="772.0" style="-fx-background-color: #000000;">
            <children>
                <ImageView fx:id="imgView" fitHeight="31.0" fitWidth="102.0" layoutX="47.0" layoutY="18.0"
                           onMouseClicked="#handleButtonActionHomePage" pickOnBounds="true" preserveRatio="true"/>
                <JFXHamburger id="jfx-hamburger" fx:id="burger" layoutX="692.0" layoutY="21.0"/>
            </children>
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
        </Pane>
        <Pane fx:id="bottomPane" focusTraversable="true" layoutY="428.0" prefHeight="62.0" prefWidth="772.0"
              style="-fx-background-color: #000000;">
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
            <children>
                <JFXButton fx:id="facebookIcon" layoutX="636.0" layoutY="19.0" prefHeight="25.0" prefWidth="24.0"/>
                <JFXButton fx:id="twitterIcon" layoutX="668.0" layoutY="19.0" prefHeight="25.0" prefWidth="24.0"/>
                <JFXButton fx:id="instagramIcon" layoutX="700.0" layoutY="19.0" prefHeight="25.0" prefWidth="24.0"/>
                <JFXButton fx:id="mailIcon" layoutX="734.0" layoutY="19.0" prefHeight="25.0" prefWidth="24.0"/>
            </children>
        </Pane>
        <Label fx:id="smhLabel" layoutX="305.0" layoutY="80.0" text="Get me somewhere" textAlignment="CENTER"
               textFill="#151928">
            <font>
                <Font name="System Bold" size="21.0"/>
            </font>
        </Label>
        <JFXButton fx:id="startBtn" layoutX="320.0" layoutY="129.0" prefHeight="38.0" prefWidth="162.0"
                   style="-fx-background-color: #151928;" text="Start" textFill="#dad1d1">
            <font>
                <Font size="16.0"/>
            </font>
        </JFXButton>
        <JFXButton fx:id="endBtn" layoutX="320.0" layoutY="193.0" prefHeight="38.0" prefWidth="162.0"
                   style="-fx-background-color: #151928;" text="End" textFill="#dad1d1">
            <font>
                <Font size="16.0"/>
            </font>
        </JFXButton>
        <JFXButton fx:id="getHomeBtn" layoutX="320.0" layoutY="366.0" onAction="#handleButtonActionHome"
                   prefHeight="38.0" prefWidth="161.0" style="-fx-background-color: #151928;" text="Get me home !"
                   textFill="#dad1d1">
            <font>
                <Font size="16.0"/>
            </font>
        </JFXButton>
        <JFXButton fx:id="nowBtn" layoutX="320.0" layoutY="244.0" prefHeight="38.0" prefWidth="78.0" text=" &gt; Now"
                   textFill="#151928">
            <font>
                <Font size="16.0"/>
            </font>
        </JFXButton>
        <JFXButton fx:id="goBtn" layoutX="419.0" layoutY="244.0" prefHeight="31.0" prefWidth="48.0"
                   style="-fx-background-color: #686f89; -fx-background-radius: 16.4, 15;" text="GO" textFill="#dad1d1">
            <font>
                <Font size="16.0"/>
            </font>
        </JFXButton>
        <JFXTextField fx:id="txtStart" alignment="TOP_LEFT" focusColor="#744b4a" layoutX="508.0" layoutY="120.0"
                      prefHeight="46.0" prefWidth="198.0" promptText="Start"
                      style="-fx-text-inner-color: #151928; -fx-prompt-text-fill: #151928; -fx-background-color: #f78f8f;"
                      unFocusColor="#24131f" visible="false">
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
            <font>
                <Font size="21.0"/>
            </font>
        </JFXTextField>
        <JFXTextField fx:id="txtEnd" alignment="TOP_LEFT" focusColor="#744b4a" layoutX="508.0" layoutY="184.0"
                      prefHeight="46.0" prefWidth="198.0" promptText="End"
                      style="-fx-text-inner-color: #151928; -fx-prompt-text-fill: #151928; -fx-background-color: #f78f8f;"
                      unFocusColor="#261e14" visible="false">
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
            <font>
                <Font size="21.0"/>
            </font>
        </JFXTextField>
        <Pane fx:id="timeInfo" layoutX="202.0" layoutY="290.0" prefHeight="62.0" prefWidth="434.0"
              style="-fx-background-color: #f78f8f;" visible="false">
            <JFXTimePicker fx:id="leaveTime" defaultColor="#151928" layoutX="334.0" layoutY="17.0" prefHeight="28.0"
                           prefWidth="85.0"/>
            <DatePicker fx:id="datePicker" layoutX="14.0" layoutY="18.0" prefHeight="26.0" prefWidth="102.0"
                        promptText="datePicker"/>
            <JFXTimePicker fx:id="arriveTime" defaultColor="#151928" layoutX="186.0" layoutY="17.0" prefHeight="28.0"
                           prefWidth="85.0"/>
            <Label fx:id="arrive" layoutX="133.0" layoutY="19.0" text="Arrive">
                <font>
                    <Font size="16.0"/>
                </font>
            </Label>
            <Label fx:id="leave" layoutX="287.0" layoutY="19.0" text="Leave">
                <font>
                    <Font size="16.0"/>
                </font>
            </Label>
        </Pane>
    </children>
</AnchorPane>
