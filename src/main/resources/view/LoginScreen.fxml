<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXCheckBox?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="425.0" prefWidth="764.0" style="-fx-background-color: #00286b;"
            xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.tubeproject.view.LoginScreen">
    <children>
        <Pane layoutX="-7.0" layoutY="-5.0" prefHeight="81.0" prefWidth="203.0" style="-fx-background-color: #000000;">
            <children>
                <ImageView fx:id="imgView" fitHeight="53.0" fitWidth="130.0" layoutX="37.0" layoutY="29.0"
                           pickOnBounds="true" preserveRatio="true"/>
            </children>
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
        </Pane>
        <JFXTextField fx:id="txtUsername" alignment="TOP_LEFT" layoutX="240.0" layoutY="137.0" prefHeight="31.0"
                      prefWidth="282.0" promptText="Username"
                      style="-fx-background-color: #00286b; -fx-text-inner-color: #dad1d1; -fx-prompt-text-fill: #dad1d1;"
                      unFocusColor="#dad1d1">
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
            <font>
                <Font size="21.0"/>
            </font>
        </JFXTextField>
        <ImageView fitHeight="67.0" fitWidth="78.0" layoutX="118.0" layoutY="149.0" pickOnBounds="true"
                   preserveRatio="true"/>
        <JFXPasswordField layoutX="240.0" layoutY="205.0" prefHeight="26.0" prefWidth="282.0" promptText="Password"
                          style="-fx-background-color: #00286b; -fx-prompt-text-fill: #dad1d1; -fx-text-inner-color: #dad1d1;"
                          unFocusColor="#dad1d1">
            <font>
                <Font size="21.0"/>
            </font>
        </JFXPasswordField>
        <JFXButton layoutX="655.0" layoutY="17.0" style="-fx-background-color: #151928;" text="Sign Up"
                   textFill="#dad1d1">
            <font>
                <Font size="16.0"/>
            </font>
        </JFXButton>
        <ImageView fx:id="imgViewUser" fitHeight="46.0" fitWidth="63.0" layoutX="178.0" layoutY="137.0"
                   pickOnBounds="true" preserveRatio="true"/>
        <ImageView fx:id="imgViewPassword" fitHeight="46.0" fitWidth="63.0" layoutX="178.0" layoutY="205.0"
                   pickOnBounds="true" preserveRatio="true"/>
        <JFXCheckBox layoutX="241.0" layoutY="282.0" prefHeight="38.0" prefWidth="139.0" text="Remember me"
                     textFill="#dad1d1" unCheckedColor="#dad1d1">
            <font>
                <Font size="16.0"/>
            </font>
        </JFXCheckBox>
        <JFXButton layoutX="410.0" layoutY="282.0" prefHeight="38.0" prefWidth="115.0"
                   style="-fx-background-color: #151928;" text="Login" textFill="#dad1d1">
            <font>
                <Font size="16.0"/>
            </font>
        </JFXButton>
        <JFXButton layoutX="306.0" layoutY="328.0" style="-fx-background-color: #00286b;" text="Forgot password ?"
                   textFill="#dad1d1" underline="true">
            <font>
                <Font size="16.0"/>
            </font>
        </JFXButton>
        <ImageView fitHeight="31.0" fitWidth="200.0" layoutX="282.0" layoutY="366.0" pickOnBounds="true"
                   preserveRatio="true"/>
    </children>
</AnchorPane>
