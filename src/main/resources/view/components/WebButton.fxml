<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      onDragDetected="#handleActionPane" prefHeight="50.0" prefWidth="205.0" xmlns="http://javafx.com/javafx/11"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tubeproject.view.component.WebButtonController">
    <children>
        <ImageView fx:id="facebookIcon" fitHeight="32.0" fitWidth="35.0" layoutX="44.0" layoutY="9.0"
                   onMouseClicked="#setUrlFacebook" pickOnBounds="true" preserveRatio="true">
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </ImageView>
        <ImageView fx:id="twitterIcon" fitHeight="32.0" fitWidth="35.0" layoutX="85.0" layoutY="9.0"
                   onMouseClicked="#setUrlTwitter" pickOnBounds="true" preserveRatio="true">
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </ImageView>
        <ImageView fx:id="instagramIcon" fitHeight="32.0" fitWidth="35.0" layoutX="127.0" layoutY="9.0"
                   onMouseClicked="#setUrlInstagram" pickOnBounds="true" preserveRatio="true">
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </ImageView>
    </children>
</Pane>
