<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="700.0" prefWidth="1200.0"
            xmlns="http://javafx.com/javafx/11" fx:controller="com.tubeproject.tool.MapManipulatorController">
    <children>
        <Pane fx:id="parentPane">
            <children>
                <ImageView fx:id="imgView" fitHeight="700.0" fitWidth="1000.0" layoutX="14.0" layoutY="14.0"
                           pickOnBounds="true"
                           preserveRatio="true"/>
            </children>
        </Pane>

        <Pane layoutX="1022.0" layoutY="14.0" prefHeight="681.0" prefWidth="164.0">
            <children>
                <ComboBox fx:id="lines" layoutX="7.0" layoutY="1.0" prefWidth="150.0"/>
                <ListView fx:id="stations" layoutX="7.0" layoutY="36.0" prefHeight="200.0" prefWidth="150.0"/>
                <Button fx:id="btnSave" layoutX="88.0" layoutY="247.0" mnemonicParsing="false" onAction="#save"
                        text="Save"/>
            </children>
        </Pane>
    </children>
</AnchorPane>
